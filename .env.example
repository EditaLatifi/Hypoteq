# Database URLs for Vercel + Supabase
# For local development, you can use DATABASE_URL
DATABASE_URL="postgresql://postgres:password@db.xxx.supabase.co:5432/postgres?sslmode=require"

# For Vercel production (required for serverless functions)
# Connection pooling URL (port 6543)
POSTGRES_PRISMA_URL="postgresql://postgres:password@db.xxx.supabase.co:6543/postgres?pgbouncer=true&connection_limit=1"

# Direct connection URL (port 5432) - used for migrations
POSTGRES_URL_NON_POOLING="postgresql://postgres:password@db.xxx.supabase.co:5432/postgres"

